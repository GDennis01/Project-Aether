config/description="Analysis of Evolved Thermal Hydrodynamics in Emission Regions"
config/version="0.1"
run/main_scene="res://scenes/world.tscn"
config/features=PackedStringArray("4.4", "Double Precision", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Hud="*res://scenes/ui/hud.tscn"
Util="*res://scripts/util.gd"
SaveManager="*res://scenes/save_manager.tscn"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1030
window/size/mode=2

